/div>\r\n\r\n          <!-- Topbar -->\r\n          <div v-if=\"layout === 'horizontal'\">\r\n            <h6 class=\"mt-3\">Topbar</h6>\r\n            <hr class=\"mt-1\" />\r\n            <b-form-radio-group\r\n              v-model=\"topbar\"\r\n              stacked\r\n              @input=\"changeTopbartype($event)\"\r\n            >\r\n              <b-form-radio value=\"dark\" class=\"mb-1\">Dark</b-form-radio>\r\n              <b-form-radio value=\"light\" class=\"mb-1\">Light</b-form-radio>\r\n            </b-form-radio-group>\r\n          </div>\r\n\r\n          <!-- Preloader -->\r\n          <h6 class=\"mt-3\">Preloader</h6>\r\n          <hr class=\"mt-1\" />\r\n\r\n          <b-form-checkbox\r\n            v-model=\"loader\"\r\n            name=\"check-button\"\r\n            switch\r\n            @input=\"changeloader\"\r\n            >Preloader</b-form-checkbox\r\n          >\r\n        </div>\r\n        <!-- Settings -->\r\n        <hr class=\"mt-0\" />\r\n        <h6 class=\"text-center mb-0\">Choose Layouts</h6>\r\n\r\n        <div class=\"p-4\">\r\n          <div class=\"mb-2\">\r\n            <router-link\r\n              to=\"//http://nazox.vuejs-light-h.themesdesign.in/\"\r\n              target=\"_blank\"\r\n            >\r\n              <img\r\n                src=\"@/assets/images/layouts/layout-4.jpg\"\r\n                class=\"img-fluid img-thumbnail\"\r\n                alt\r\n              />\r\n            </router-link>\r\n          </div>\r\n\r\n          <div class=\"mb-2\">\r\n            <router-link\r\n              to=\"//http://nazox.vuejs-dark-h.themesdesign.in/\"\r\n              target=\"_blank\"\r\n            >\r\n              <img\r\n                src=\"@/assets/images/layouts/layout-5.jpg\"\r\n                class=\"img-fluid img-thumbnail\"\r\n                alt\r\n              />\r\n            </router-link>\r\n          </div>\r\n\r\n          <div class=\"mb-2\">\r\n            <router-link\r\n              to